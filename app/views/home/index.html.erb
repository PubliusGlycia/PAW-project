<div id="app-container"></div>

<div class="main">
    <% if current_user %>
        <%= button_to 'Sign out', destroy_user_session_path, 
            method: :delete %>
        <%= button_to 'Pages', pages_path, method: :get %>
    <% else %>
        <%= button_to 'Sign up', new_user_registration_path, method: :get %>
        <%= button_to 'Log in', new_user_session_path, method: :get %>
 </div>
<% end %>